<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.panjy.servicemetricsplatform.mapper.WechatMemberMapper">
<!-- 查询非职员用户 -->
    <select id="findAllUser" resultType="java.lang.String">
        SELECT m.wechat_id
        FROM wechat_member m
        LEFT JOIN wechat_account a
            ON m.wechat_id = a.wechat_id
        WHERE a.wechat_id IS NULL
#         LIMIT 20
    </select>
</mapper>